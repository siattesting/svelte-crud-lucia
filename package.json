		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.4.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "^2.0.1",
		"@lucia-auth/adapter-prisma": "^3.0.1",
		"@nanostores/query": "^0.2.4",
